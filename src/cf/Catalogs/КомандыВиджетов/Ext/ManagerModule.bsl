// Создаёт команду "Открыть реестр".
//
// Параметры:
//  Реестр - СправочникСсылка.Реестры.
// 
// Возвращаемое значение:
//  СправочникСсылка.КомандыВиджетов - Команда "Открыть реестр".
//
Функция СоздатьКомандуОткрытьРеестр(Реестр) Экспорт
	
	УстановитьПривилегированныйРежим(Истина);
	
	Действие = Перечисления.ДействияВиджетов.ОткрытьРеестр;
	
	ДанныеЗаполнения = Новый Структура;
	ДанныеЗаполнения.Вставить("Наименование",
		СтрШаблон("%1 ""%2""",
			Действие,
			Реестр.УникальныйИдентификатор()));
	ДанныеЗаполнения.Вставить("Источник", Реестр);
	ДанныеЗаполнения.Вставить("Действие", Действие);
	
	КомандаОбъект = СоздатьЭлемент();
	КомандаОбъект.Заполнить(ДанныеЗаполнения);
	КомандаОбъект.Записать();
	
	Возврат КомандаОбъект.Ссылка;
	
КонецФункции

// Создаёт команду "Создать элемент реестра".
//
// Параметры:
//  Реестр - СправочникСсылка.Реестры.
// 
// Возвращаемое значение:
//  СправочникСсылка.КомандыВиджетов - Команда "Создать элемент реестра".
//
Функция СоздатьКомандуСоздатьЭлементРеестра(Реестр) Экспорт
	
	УстановитьПривилегированныйРежим(Истина);
	
	Действие = Перечисления.ДействияВиджетов.СоздатьЭлементРеестра;
	
	ДанныеЗаполнения = Новый Структура;
	ДанныеЗаполнения.Вставить("Наименование",
		СтрШаблон("%1 ""%2""",
			Действие,
			Реестр.УникальныйИдентификатор()));
	ДанныеЗаполнения.Вставить("Источник", Реестр);
	ДанныеЗаполнения.Вставить("Действие", Действие);
	ДанныеЗаполнения.Вставить("Картинка", "ВиджетБелыйСоздать");
	
	КомандаОбъект = СоздатьЭлемент();
	КомандаОбъект.Заполнить(ДанныеЗаполнения);
	КомандаОбъект.Записать();
	
	Возврат КомандаОбъект.Ссылка;
	
КонецФункции

// Данные команды виджета.
// 
// Возвращаемое значение:
//  Структура - Структура данные команды:
//   * КомандаВиджета - Строка.
//   * Заголовок - Строка.
//   * Подсказка - Строка.
//   * Картинка - Картинка, Неопределено.
//   * Источник - Строка, СправочникСсылка.Реестры.
//   * Действие - Структура, Неопределено.
//   * Условие - СправочникСсылка.АлгоритмыПроверки.
//   * ТипСтруктуры - Строка.
//
Функция НовыйДанныеКоманды() Экспорт
	
	ДанныеКоманды = Новый Структура;
	ДанныеКоманды.Вставить("КомандаВиджета", "");
	ДанныеКоманды.Вставить("Заголовок", "");
	ДанныеКоманды.Вставить("Подсказка", "");
	ДанныеКоманды.Вставить("Картинка");
	ДанныеКоманды.Вставить("Источник");
	ДанныеКоманды.Вставить("Действие");
	ДанныеКоманды.Вставить("Условие", Справочники.АлгоритмыПроверки.ПустаяСсылка());
	ДанныеКоманды.Вставить("ТипСтруктуры", "ДанныеКоманды");
	
	Возврат ДанныеКоманды;
	
КонецФункции

#КонецОбласти

#Область ОбработчикиСобытий

Процедура ОбработкаПолученияПолейПредставления(Поля, СтандартнаяОбработка)
	
	СтандартнаяОбработка = Ложь;
	Поля.Добавить("Наименование");
	Поля.Добавить("Действие");
	Поля.Добавить("Источник");
	
	МультиязычностьКлиентСервер.ОбработкаПолученияПолейПредставления(Поля, СтандартнаяОбработка);
	
КонецПроцедуры

Процедура ОбработкаПолученияПредставления(Данные, Представление, СтандартнаяОбработка)
	
	СтандартнаяОбработка = Ложь;
	
	МультиязычностьКлиентСервер.ОбработкаПолученияПредставления(Данные, Представление, СтандартнаяОбработка);
	Если ЗначениеЗаполнено(Представление) Тогда
		Данные.Наименование = Представление;
	КонецЕсли;
	
	Если Данные.Действие = Перечисления.ДействияВиджетов.СоздатьЭлементРеестра Тогда
		
		Представление = СтрШаблон("%1 ""%2""", Данные.Действие, Строка(Данные.Источник));
		
	ИначеЕсли Данные.Действие = Перечисления.ДействияВиджетов.ОткрытьРеестр Тогда
		
		Представление = Строка(Данные.Источник);
		
	Иначе
		
		Представление = Данные.Наименование;
		
	КонецЕсли;
	
КонецПроцедуры

Процедура ОбработкаПолученияДанныхВыбора(ДанныеВыбора, Параметры, СтандартнаяОбработка)
	
	СтандартнаяОбработка = Ложь;
	
	Запрос = Новый Запрос;
	
	Запрос.УстановитьПараметр("СтрокаПоиска", ?(Параметры.СтрокаПоиска = Неопределено, "", Параметры.СтрокаПоиска) + "%");
	
	Запрос.Текст =
	"ВЫБРАТЬ
	|	КомандыВиджетов.Ссылка КАК Ссылка
	|ИЗ
	|	Справочник.КомандыВиджетов КАК КомандыВиджетов
	|ГДЕ
	|	НЕ КомандыВиджетов.ПометкаУдаления
	|	И КомандыВиджетов.Наименование ПОДОБНО &СтрокаПоиска
	|
	|УПОРЯДОЧИТЬ ПО
	|	КомандыВиджетов.Наименование";
	
	ОбъектМетаданных = Метаданные.Справочники.КомандыВиджетов;
	Если Не МультиязычностьПовтИсп.КонфигурацияИспользуетТолькоОдинЯзык(ОбъектМетаданных.ТабличныеЧасти.Найти("Представления") = Неопределено) Тогда
		
		Если МультиязычностьСервер.ИспользуетсяПервыйДополнительныйЯзык() Тогда
			Запрос.Текст = СтрЗаменить(Запрос.Текст,
				"КомандыВиджетов.Наименование ПОДОБНО &СтрокаПоиска",
				"(КомандыВиджетов.Наименование ПОДОБНО &СтрокаПоиска ИЛИ КомандыВиджетов.НаименованиеЯзык1 ПОДОБНО &СтрокаПоиска)");
		КонецЕсли;
		
	КонецЕсли;
	
	ДанныеВыбора = Новый СписокЗначений;
	Выборка = Запрос.Выполнить().Выбрать();
	Пока Выборка.Следующий() Цикл
		ДанныеВыбора.Добавить(Выборка.Ссылка);
	КонецЦикла;
	
КонецПроцедуры

#КонецОбласти

#Область СлужебныеПроцедурыИФункции

// Возвращает данные поставялемых команд.
Функция СформироватьДанныеКоманды(Выборка)
	
	ДанныеКоманды = НовыйДанныеКоманды();
	ДанныеКоманды.КомандаВиджета = Выборка.Ссылка;
	ДанныеКоманды.Заголовок = Выборка.Заголовок;
	ДанныеКоманды.Подсказка = Выборка.Подсказка;
	ДанныеКоманды.Условие = Выборка.Условие;
	ДанныеКоманды.Источник = ?(ЗначениеЗаполнено(Выборка.ИсточникСтрока),
		Выборка.ИсточникСтрока,
		Выборка.Источник);

	КонфигурацияИспользуетТолькоОдинЯзык = МультиязычностьПовтИсп.КонфигурацияИспользуетТолькоОдинЯзык(Ложь);
	Если Не КонфигурацияИспользуетТолькоОдинЯзык И Не МультиязычностьСервер.ЭтоОсновнойЯзык() Тогда
		ДанныеКоманды.Заголовок = Выборка.НаименованиеЯзык1; // обход
	КонецЕсли;

	Если ЗначениеЗаполнено(Выборка.Картинка) Тогда
		ДанныеКоманды.Картинка = БиблиотекаКартинок[Выборка.Картинка];
	КонецЕсли;
	
	Если ЗначениеЗаполнено(Выборка.Действие)
		И Выборка.Действие <> Перечисления.ДействияВиджетов.ОбработатьИсточник Тогда
		ДействиеВиджета = Выборка.Действие;
		ВыраженияПараметровДействия = Новый Массив;
		ВыраженияПараметровДействия.Добавить(Выборка.ПараметрДействия1);
		ВыраженияПараметровДействия.Добавить(Выборка.ПараметрДействия2);
		ДанныеКоманды.Действие = Перечисления.ДействияВиджетов.Действие(
			ДействиеВиджета,
			ДанныеКоманды.Источник,
			ВыраженияПараметровДействия);
	КонецЕсли;
	
	Если ТипЗнч(ДанныеКоманды.Источник) = Тип("СправочникСсылка.Реестры")
		И Не ЗначениеЗаполнено(Выборка.Заголовок) Тогда
		
		ДанныеКоманды.Заголовок = Выборка.ПредставлениеРеестра;
		
	КонецЕсли;
	
	Возврат ДанныеКоманды;
	
КонецФункции

// Инициализирует данные команды.
Процедура ИнициализироватьКоманду(ДанныеКоманды)
	
	Если ЗначениеЗаполнено(ДанныеКоманды.Источник)
		И ТипЗнч(ДанныеКоманды.Источник) = Тип("Строка") Тогда
		
		МетаданныеИсточника = Метаданные.НайтиПоПолномуИмени(ДанныеКоманды.Источник);
		
		Если Не ЗначениеЗаполнено(ДанныеКоманды.Заголовок) Тогда
			
			Если ОбщегоНазначенияКлиентСервер.ЕстьРеквизитИлиСвойствоОбъекта(МетаданныеИсточника, "ПредставлениеСписка")
				И ЗначениеЗаполнено(МетаданныеИсточника.ПредставлениеСписка) Тогда
				ДанныеКоманды.Заголовок = МетаданныеИсточника.ПредставлениеСписка;
			Иначе
				ДанныеКоманды.Заголовок = МетаданныеИсточника.Синоним;
			КонецЕсли;
			
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(ДанныеКоманды.Подсказка)
			И ОбщегоНазначенияКлиентСервер.ЕстьРеквизитИлиСвойствоОбъекта(МетаданныеИсточника, "Подсказка") Тогда
			ДанныеКоманды.Подсказка = МетаданныеИсточника.Подсказка;
		КонецЕсли;
		
		Если ДанныеКоманды.Действие = Неопределено Тогда
			ДанныеКоманды.Действие = РаботаСВиджетами.ДействиеИсточника(ДанныеКоманды.Источник);
		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры

// Проверяет доступность команды.
Функция КомандаДоступна(ДанныеКоманды)
	
	КомандаДоступна = РаботаСВиджетами.ИсточникДоступен(ДанныеКоманды.Источник)
		И РаботаСРабочимСтолом.ПроверитьУсловие(ДанныеКоманды.Условие);
	
	Возврат КомандаДоступна;
	
КонецФункции

