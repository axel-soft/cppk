// Формирует данные всех команд.
// 
// Возвращаемое значение:
// 	Соответствие из КлючИЗначение - Данные команд:
// 	 * Ключ - СправочникСсылка.КомандыВиджетов.
// 	 * Значение - Структура - Данные команды. См. Справочники.КомандыВиджетов.НовыйДанныеКоманды.
//
Функция ДанныеВсехКоманд() Экспорт
	
	Возврат ДанныеКоманд();
	
КонецФункции

// Формирует данные команд.
//
// Параметры:
//  Команда - СправочникСсылка.КомандыВиджетов.
// 
// Возвращаемое значение:
//  Соответствие из КлючИЗначение - Данные команд:
//   * Ключ - СправочникСсылка.КомандыВиджетов.
//   * Значение - Структура - Данные команды. См. Справочники.КомандыВиджетов.НовыйДанныеКоманды.
//
Функция ДанныеКоманд(КомандыВиджетов = Неопределено) Экспорт
	
	ДанныеВсехКоманд = Новый Соответствие;
	
	Запрос = Новый Запрос(
		"ВЫБРАТЬ
		|	КомандыВиджетов.ВерсияДанных,
		|	КомандыВиджетов.Действие,
		|	КомандыВиджетов.Заголовок,
		|	КомандыВиджетов.ИмяПредопределенныхДанных,
		|	КомандыВиджетов.Источник,
		|	КомандыВиджетов.ИсточникСтрока,
		|	КомандыВиджетов.Картинка,
		|	КомандыВиджетов.Наименование,
		|	КомандыВиджетов.НаименованиеЯзык1,
		|	КомандыВиджетов.ПараметрДействия1,
		|	КомандыВиджетов.ПараметрДействия2,
		|	КомандыВиджетов.Подсказка,
		|	КомандыВиджетов.ПометкаУдаления,
		|	КомандыВиджетов.Предопределенный,
		|	КомандыВиджетов.Представление,
		|	КомандыВиджетов.Ссылка,
		|	КомандыВиджетов.Условие,
		|	ВЫБОР
		|		КОГДА ТИПЗНАЧЕНИЯ(КомандыВиджетов.Источник) = ТИП(Справочник.Реестры)
		|			ТОГДА ПРЕДСТАВЛЕНИЕ(ВЫРАЗИТЬ(КомандыВиджетов.Источник КАК Справочник.Реестры))
		|		ИНАЧЕ """"
		|	КОНЕЦ КАК ПредставлениеРеестра
		|ИЗ
		|	Справочник.КомандыВиджетов КАК КомандыВиджетов
		|ГДЕ
		|	КомандыВиджетов.Ссылка В (&КомандыВиджетов)
		|	И КомандыВиджетов.ПометкаУдаления = ЛОЖЬ");
	
	Если КомандыВиджетов <> Неопределено Тогда
		Запрос.УстановитьПараметр("КомандыВиджетов", КомандыВиджетов);
	Иначе
		Запрос.Текст = СтрЗаменить(Запрос.Текст, "КомандыВиджетов.Ссылка В (&КомандыВиджетов)", "");
		Запрос.Текст = СтрЗаменить(Запрос.Текст, "И КомандыВиджетов.ПометкаУдаления = ЛОЖЬ", "КомандыВиджетов.ПометкаУдаления = ЛОЖЬ");
	КонецЕсли;
	
	Выборка = Запрос.Выполнить().Выбрать();
	Пока Выборка.Следующий() Цикл
		
		Попытка
			
			ДанныеКоманды = СформироватьДанныеКоманды(Выборка);
			
		Исключение
			
			ИнформацияОбОшибке = ИнформацияОбОшибке();
			ТекстЖурналаРегистрации = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке);
			
			СобытиеЖурналаРегистрации = РаботаСВиджетами.СобытиеЖурналаРегистрации();
			
			ЗаписьЖурналаРегистрации(
				СобытиеЖурналаРегистрации,
				УровеньЖурналаРегистрации.Ошибка,,,
				ТекстЖурналаРегистрации);
			
			Продолжить;
			
		КонецПопытки;
		
		Если Не КомандаДоступна(ДанныеКоманды) Тогда
			Продолжить;
		КонецЕсли;
		
		ИнициализироватьКоманду(ДанныеКоманды);
	
		ДанныеВсехКоманд[Выборка.Ссылка] = ДанныеКоманды;
		
	КонецЦикла;
	
	Возврат ДанныеВсехКоманд;
	
КонецФункции

// Данные команды виджета.
// 
// Параметры:
//  Команда - СправочникСсылка.КомандыВиджетов.
// 
// Возвращаемое значение:
//  Структура - Данные команды. См. Справочники.КомандыВиджетов.НовыйДанныеКоманды.
//
Функция ДанныеКоманды(Команда) Экспорт
	
	КомандыВиджетов = ОбщегоНазначенияКлиентСервер.ЗначениеВМассиве(Команда);
	ДанныеКоманд = ДанныеКоманд(КомандыВиджетов);
	ДанныеКоманды = ДанныеКоманд[Команда];
	
	Возврат ДанныеКоманды;
	
КонецФункции

// Данные команды "Еще".
// 
// Параметры:
//  ДанныеКомандМеню - Массив.
// 
// Возвращаемое значение:
//  Структура, Неопределено - Данные команды "Еще". См. Справочники.КомандыВиджетов.НовыйДанныеКоманды.
//
Функция ДанныеКомандыЕще(ДанныеКомандМеню) Экспорт
	
	Если ДанныеКомандМеню.Количество() = 0 Тогда
		Возврат Неопределено;
	КонецЕсли;
	
	ДанныеКоманды = НовыйДанныеКоманды();
	ДанныеКоманды.Заголовок = НСтр("ru = 'Еще'");
	ДанныеКоманды.Подсказка = НСтр("ru = 'Показать остальные команды'");
	
	Меню = Новый Массив;
	Для Каждого ДанныеКомандыМеню Из ДанныеКомандМеню Цикл
		ЭлементМеню = Новый Структура;
		ЭлементМеню.Вставить("Заголовок", ДанныеКомандыМеню.Заголовок);
		ЭлементМеню.Вставить("Картинка", ДанныеКомандыМеню.Картинка);
		ЭлементМеню.Вставить("Действие", ДанныеКомандыМеню.Действие);
		Меню.Добавить(ЭлементМеню);
	КонецЦикла;
	ДанныеКоманды.Действие = Перечисления.ДействияВиджетов.ПоказатьВыборИзМеню(Меню);
	
	Возврат ДанныеКоманды;
	
КонецФункции

