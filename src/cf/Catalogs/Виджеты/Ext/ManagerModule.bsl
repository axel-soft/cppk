#Если Сервер Или ТолстыйКлиентОбычноеПриложение Или ВнешнееСоединение Тогда

#Область ПрограммныйИнтерфейс

// Получает данные виджета.
// 
// Возвращаемое значение:
// 	Структура - Данные виджета. См. Справочник.Виджеты.НовыйДанныеВиджета.
//
Функция ДанныеВиджета(Виджет) Экспорт
	
	Виджеты = ОбщегоНазначенияКлиентСервер.ЗначениеВМассиве(Виджет);
	ДанныеВиджетов = ДанныеВиджетов(Виджеты);
	ДанныеВиджета = ДанныеВиджетов[Виджет];
	
	Возврат ДанныеВиджета;
	
КонецФункции

// Получает данные виджета.
// 
// Возвращаемое значение:
// 	Соответствие из КлючИЗначение: - Данные виджетов:
// 	 * Ключ - Строка - Виджет.
// 	 * Значение -  Структура - Данные виджета. См. Справочники.Виджеты.НовыйДанныеВиджета.
//
Функция ДанныеВиджетов(Виджеты = Неопределено) Экспорт
	
	ДанныеВиджетов = Новый Соответствие;
	
	Если Виджеты <> Неопределено Тогда
		
		ВиджетыВключаяВторойУровень = Новый Массив;
		ОбщегоНазначенияКлиентСервер.ДополнитьМассив(ВиджетыВключаяВторойУровень, Виджеты);
		
		Запрос = Новый Запрос(
			"ВЫБРАТЬ
			|	ВиджетыВложенныеВиджеты.Виджет КАК Виджет
			|ИЗ
			|	Справочник.Виджеты.ВложенныеВиджеты КАК ВиджетыВложенныеВиджеты
			|ГДЕ
			|	ВиджетыВложенныеВиджеты.Ссылка В (&Виджеты)
			|
			|ОБЪЕДИНИТЬ ВСЕ
			|
			|ВЫБРАТЬ
			|	ВиджетыКомпоновка.Виджет
			|ИЗ
			|	Справочник.Виджеты.Компоновка КАК ВиджетыКомпоновка
			|ГДЕ
			|	ВиджетыКомпоновка.Ссылка В (&Виджеты)");
		
		Запрос.УстановитьПараметр("Виджеты", Виджеты);
		
		Выборка = Запрос.Выполнить().Выбрать();
		Пока Выборка.Следующий() Цикл
			
			Если Не ЗначениеЗаполнено(Выборка.Виджет)
				Или ВиджетыВключаяВторойУровень.Найти(Выборка.Виджет) <> Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			ВиджетыВключаяВторойУровень.Добавить(Выборка.Виджет);
			
		КонецЦикла;
			
	Иначе
		
		ВиджетыВключаяВторойУровень = Неопределено;
		
	КонецЕсли;
	
	Запрос = Новый Запрос(
		"ВЫБРАТЬ
		|	Виджеты.Ссылка,
		|	Виджеты.ВерсияДанных,
		|	Виджеты.ПометкаУдаления,
		|	Виджеты.Наименование,
		|	Виджеты.НаименованиеЯзык1,
		|	Виджеты.Вид,
		|	Виджеты.ВысотаСхемы,
		|	Виджеты.ДоступенДляВыбора,
		|	Виджеты.Заголовок,
		|	Виджеты.ЗаголовокЯзык1,
		|	Виджеты.Картинка,
		|	Виджеты.МК_Картинка,
		|	Виджеты.КомандаНастройки,
		|	Виджеты.МаксимальноеЗначение,
		|	Виджеты.МинимальноеЗначение,
		|	Виджеты.ПоложениеЗаголовка,
		|	Виджеты.ПредставлениеПустого,
		|	Виджеты.ПредставлениеПустогоЯзык1,
		|	Виджеты.ТипДиаграммы,
		|	Виджеты.Цвет,
		|	Виджеты.ШиринаСхемы,
		|	Виджеты.БлокиНавигации.(
		|		Ссылка,
		|		НомерСтроки,
		|		БлокНавигации),
		|	Виджеты.ВложенныеВиджеты.(
		|		Ссылка,
		|		НомерСтроки,
		|		Виджет),
		|	Виджеты.ДополнительныеКоманды.(
		|		Ссылка,
		|		НомерСтроки,
		|		Команда),
		|	Виджеты.ДополнительныеПоказатели.(
		|		Ссылка,
		|		НомерСтроки,
		|		Показатель),
		|	Виджеты.Команды.(
		|		Ссылка,
		|		НомерСтроки,
		|		Команда),
		|	Виджеты.Компоновка.(
		|		Ссылка,
		|		НомерСтроки,
		|		ВертикальныйИнтервал,
		|		Виджет,
		|		Высота,
		|		ГоризонтальноеПоложениеВГруппе,
		|		ГоризонтальныйИнтервал,
		|		Группировка,
		|		Имя,
		|		МаксимальныйОтступ,
		|		Отступ,
		|		РастягиватьПоВертикали,
		|		РастягиватьПоГоризонтали,
		|		Родитель,
		|		Тип,
		|		ЦветВиджета,
		|		Ширина),
		|	Виджеты.ОсновныеПоказатели.(
		|		Ссылка,
		|		НомерСтроки,
		|		Показатель),
		|	Виджеты.ПоказателиЗаголовка.(
		|		Ссылка,
		|		НомерСтроки,
		|		Показатель),
		|	Виджеты.Предопределенный,
		|	Виджеты.ИмяПредопределенныхДанных,
		|	Виджеты.Представление
		|ИЗ
		|	Справочник.Виджеты КАК Виджеты
		|ГДЕ
		|	Виджеты.Ссылка В (&ВиджетыВключаяВторойУровень)
		|	И Виджеты.ПометкаУдаления = ЛОЖЬ");
	
	Если ВиджетыВключаяВторойУровень <> Неопределено Тогда
		Запрос.УстановитьПараметр("ВиджетыВключаяВторойУровень", ВиджетыВключаяВторойУровень);
	Иначе
		Запрос.Текст = СтрЗаменить(Запрос.Текст, "Виджеты.Ссылка В (&ВиджетыВключаяВторойУровень)", "");
		Запрос.Текст = СтрЗаменить(Запрос.Текст, "И Виджеты.ПометкаУдаления = ЛОЖЬ", "Виджеты.ПометкаУдаления = ЛОЖЬ");
	КонецЕсли;
	
	Выборка = Запрос.Выполнить().Выбрать();
	
	// Получим вспомогательные данные для формирования виджетов.
	БлокиНавигации = Новый Массив;
	КомандыВиджетов = Новый Массив;
	ПоказателиВиджетов = Новый Массив;
	
	Пока Выборка.Следующий() Цикл
		
		СтрокиБлоковНавигации = Выборка.БлокиНавигации.Выгрузить();
		Для Каждого СтрокаБлокаНавигации Из СтрокиБлоковНавигации Цикл
			
			Если ЗначениеЗаполнено(СтрокаБлокаНавигации.БлокНавигации)
				И БлокиНавигации.Найти(СтрокаБлокаНавигации.БлокНавигации) <> Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			БлокиНавигации.Добавить(СтрокаБлокаНавигации.БлокНавигации);
			
		КонецЦикла;
		
		Команды = Выборка.Команды.Выгрузить();
		Для Каждого СтрокаКоманды Из Команды Цикл
			
			Если ЗначениеЗаполнено(СтрокаКоманды.Команда)
				И КомандыВиджетов.Найти(СтрокаКоманды.Команда) <> Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			КомандыВиджетов.Добавить(СтрокаКоманды.Команда);
			
		КонецЦикла;
		
		ДополнительныеКоманды = Выборка.ДополнительныеКоманды.Выгрузить();
		Для Каждого СтрокаКоманды Из ДополнительныеКоманды Цикл
			
			Если ЗначениеЗаполнено(СтрокаКоманды.Команда)
				И КомандыВиджетов.Найти(СтрокаКоманды.Команда) <> Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			КомандыВиджетов.Добавить(СтрокаКоманды.Команда);
			
		КонецЦикла;
		
		ОсновныеПоказатели = Выборка.ОсновныеПоказатели.Выгрузить();
		Для Каждого СтрокаПоказателя Из ОсновныеПоказатели Цикл
			
			Если ЗначениеЗаполнено(СтрокаПоказателя.Показатель)
				И ПоказателиВиджетов.Найти(СтрокаПоказателя.Показатель) <> Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			ПоказателиВиджетов.Добавить(СтрокаПоказателя.Показатель);
			
		КонецЦикла;
		
		ПоказателиЗаголовка = Выборка.ПоказателиЗаголовка.Выгрузить();
		Для Каждого СтрокаПоказателя Из ПоказателиЗаголовка Цикл
			
			Если ЗначениеЗаполнено(СтрокаПоказателя.Показатель)
				И ПоказателиВиджетов.Найти(СтрокаПоказателя.Показатель) <> Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			ПоказателиВиджетов.Добавить(СтрокаПоказателя.Показатель);
			
		КонецЦикла;
		
		ДополнительныеПоказатели = Выборка.ДополнительныеПоказатели.Выгрузить();
		Для Каждого СтрокаПоказателя Из ДополнительныеПоказатели Цикл
			
			Если ЗначениеЗаполнено(СтрокаПоказателя.Показатель)
				И ПоказателиВиджетов.Найти(СтрокаПоказателя.Показатель) <> Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			ПоказателиВиджетов.Добавить(СтрокаПоказателя.Показатель);
			
		КонецЦикла;
		
	КонецЦикла;
	
	КомандыИПоказателиБлоков = Справочники.БлокиНавигации.КомандыИПоказатели(БлокиНавигации);
	ОбщегоНазначенияКлиентСервер.ДополнитьМассив(КомандыВиджетов, КомандыИПоказателиБлоков.Команды);
	ОбщегоНазначенияКлиентСервер.ДополнитьМассив(ПоказателиВиджетов, КомандыИПоказателиБлоков.Показатели);
	
	ДанныеКоманд = Справочники.КомандыВиджетов.ДанныеКоманд(КомандыВиджетов);
	
	ДанныеПоказателей = Справочники.ПоказателиВиджетов.ДанныеПоказателей(ПоказателиВиджетов);
	
	СУчетомНастроек = Истина;
	ДанныеБлоковНавигации = Справочники.БлокиНавигации.ДанныеБлоковНавигации(
		БлокиНавигации,
		СУчетомНастроек,
		ДанныеКоманд,
		ДанныеПоказателей);
	
	// Сформируем данные виджетов.
	Выборка.Сбросить();
	Пока Выборка.Следующий() Цикл
		
		ДанныеВиджета = СформироватьДанныеВиджета(Выборка, ДанныеБлоковНавигации, ДанныеКоманд, ДанныеПоказателей);
		Если Не ДанныеВиджета.ВиджетДоступен Тогда
			Продолжить;
		КонецЕсли;
		
		ИнициализироватьВиджет(ДанныеВиджета);
		Если Не ДанныеВиджета.ВиджетДоступен Тогда
			Продолжить;
		КонецЕсли;
		
		ДанныеВиджетов[ДанныеВиджета.Ссылка] = ДанныеВиджета;
		
	КонецЦикла;

	// Удалим данные виджетов, у которых нет ни одного вложенного доступного виджета.
	Выборка.Сбросить();
	Пока Выборка.Следующий() Цикл
		
		ДанныеВиджета = ДанныеВиджетов[Выборка.Ссылка];
		Если ДанныеВиджета = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		ЕстьВложенныеВиджеты = Ложь;
		ЕстьДоступныеВложенныеВиджеты = Ложь;
		ВложенныеВиджеты = Выборка.ВложенныеВиджеты.Выгрузить();
		ВложенныеВиджеты.Сортировать("НомерСтроки Возр");
		Для Каждого СтрокаВиджета Из ВложенныеВиджеты Цикл
			
			ЕстьВложенныеВиджеты = Истина;
			ДанныеВложенногоВиджета = ДанныеВиджетов[СтрокаВиджета.Виджет];
			Если ДанныеВложенногоВиджета = Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			ЕстьДоступныеВложенныеВиджеты = Истина;
			ДанныеВиджета.ВложенныеВиджеты.Добавить(ДанныеВложенногоВиджета);
			
		КонецЦикла;
		Если ЕстьВложенныеВиджеты И Не ЕстьДоступныеВложенныеВиджеты Тогда
			ДанныеВиджетов.Удалить(Выборка.Ссылка);
		КонецЕсли;
		
	КонецЦикла;
	
	Возврат ДанныеВиджетов;
	
КонецФункции

// Формирует данные всех виджетов.
// 
// Возвращаемое значение:
// 	Соответствие из КлючИЗначение - Данные виджетов:
// 	 * Ключ - Строка - Виджет.
// 	 * Значение -  Структура - Данные виджета. См. Справочники.Виджеты.НовыйДанныеВиджета.
//
Функция ДанныеВсехВиджетов() Экспорт
	
	ДанныеВсехВиджетов = ДанныеВиджетов();
	
	Возврат ДанныеВсехВиджетов;
	
КонецФункции

// Заполняет поставляемые данные.
// 
Процедура ЗаполнитьПоставляемыеДанные() Экспорт
	
	// Руководителя
	ВиджетОбъект = БлокНавигацииРуководителя.ПолучитьОбъект();
	ВиджетОбъект.Наименование = НСтр("ru = 'Руководителя'");
	ВиджетОбъект.Вид = Перечисления.ВидыВиджетов.БлокНавигации;
	ВиджетОбъект.ВысотаСхемы = 3;
	ВиджетОбъект.ДоступенДляВыбора = Ложь;
	ВиджетОбъект.Заголовок = "";
	ВиджетОбъект.Картинка = "";
	ВиджетОбъект.КомандаНастройки = "e1cib/command/РегистрСведений.НастройкиБлоковНавигации.Команда.НастроитьБлокиНавигации";
	ВиджетОбъект.МаксимальноеЗначение = 0;
	ВиджетОбъект.МинимальноеЗначение = 0;
	ВиджетОбъект.ПоложениеЗаголовка = "";
	ВиджетОбъект.ПредставлениеПустого = "";
	ВиджетОбъект.ТипДиаграммы = "";
	ВиджетОбъект.Цвет = Перечисления.ЦветаВиджетов.Авто;
	ВиджетОбъект.ШиринаСхемы = 1;
	ВиджетОбъект.БлокиНавигации.Очистить();
	СтрокаБлока = ВиджетОбъект.БлокиНавигации.Добавить();
	СтрокаБлока.БлокНавигации = Справочники.БлокиНавигации.РеестрыРуководителя;
	СтрокаБлока = ВиджетОбъект.БлокиНавигации.Добавить();
	СтрокаБлока.БлокНавигации = Справочники.БлокиНавигации.БлокСправочники;
	СтрокаБлока = ВиджетОбъект.БлокиНавигации.Добавить();
	СтрокаБлока.БлокНавигации = Справочники.БлокиНавигации.Настройки;
	СтрокаБлока = ВиджетОбъект.БлокиНавигации.Добавить();
	СтрокаБлока.БлокНавигации = Справочники.БлокиНавигации.Классификаторы;
	СтрокаБлока = ВиджетОбъект.БлокиНавигации.Добавить();
	СтрокаБлока.БлокНавигации = Справочники.БлокиНавигации.Предприятие;
	
	ВиджетОбъект.ВложенныеВиджеты.Очистить();
	
	ВиджетОбъект.ДополнительныеКоманды.Очистить();
	
	ВиджетОбъект.ДополнительныеПоказатели.Очистить();
	
	ВиджетОбъект.Команды.Очистить();
	
	ВиджетОбъект.Компоновка.Очистить();
	
	ВиджетОбъект.ОсновныеПоказатели.Очистить();
	
	ВиджетОбъект.ПоказателиЗаголовка.Очистить();
	
	ОбновлениеИнформационнойБазы.ЗаписатьДанные(ВиджетОбъект);
	
	// Сотрудника
	ВиджетОбъект = БлокНавигацииСотрудника.ПолучитьОбъект();
	ВиджетОбъект.Наименование = НСтр("ru = 'Сотрудника'");
	ВиджетОбъект.Вид = Перечисления.ВидыВиджетов.БлокНавигации;
	ВиджетОбъект.ВысотаСхемы = 3;
	ВиджетОбъект.ДоступенДляВыбора = Ложь;
	ВиджетОбъект.Заголовок = "";
	ВиджетОбъект.Картинка = "";
	ВиджетОбъект.КомандаНастройки = "e1cib/command/РегистрСведений.НастройкиБлоковНавигации.Команда.НастроитьБлокиНавигации";
	ВиджетОбъект.МаксимальноеЗначение = 0;
	ВиджетОбъект.МинимальноеЗначение = 0;
	ВиджетОбъект.ПоложениеЗаголовка = "";
	ВиджетОбъект.ПредставлениеПустого = "";
	ВиджетОбъект.ТипДиаграммы = "";
	ВиджетОбъект.Цвет = Перечисления.ЦветаВиджетов.Авто;
	ВиджетОбъект.ШиринаСхемы = 1;
	ВиджетОбъект.БлокиНавигации.Очистить();
	СтрокаБлока = ВиджетОбъект.БлокиНавигации.Добавить();
	СтрокаБлока.БлокНавигации = Справочники.БлокиНавигации.РеестрыСотрудника;
	СтрокаБлока = ВиджетОбъект.БлокиНавигации.Добавить();
	СтрокаБлока.БлокНавигации = Справочники.БлокиНавигации.БлокСправочники;
	СтрокаБлока = ВиджетОбъект.БлокиНавигации.Добавить();
	СтрокаБлока.БлокНавигации = Справочники.БлокиНавигации.Настройки;
	СтрокаБлока = ВиджетОбъект.БлокиНавигации.Добавить();
	СтрокаБлока.БлокНавигации = Справочники.БлокиНавигации.Классификаторы;
	СтрокаБлока = ВиджетОбъект.БлокиНавигации.Добавить();
	СтрокаБлока.БлокНавигации = Справочники.БлокиНавигации.Предприятие;
	
	ВиджетОбъект.ВложенныеВиджеты.Очистить();
	
