Процедура ОбработкаПолученияДанныхВыбора(ДанныеВыбора, Параметры, СтандартнаяОбработка)
	
	СтандартнаяОбработка = Ложь;
	
	Запрос = Новый Запрос;
	
	Запрос.УстановитьПараметр("СтрокаПоиска", ?(Параметры.СтрокаПоиска = Неопределено, "", Параметры.СтрокаПоиска) + "%");
	
	Запрос.Текст =
	"ВЫБРАТЬ
	|	ВидыЗадач.Ссылка КАК Ссылка
	|ИЗ
	|	Справочник.ВидыЗадач КАК ВидыЗадач
	|ГДЕ
	|	НЕ ВидыЗадач.ПометкаУдаления
	|	И ВидыЗадач.Наименование ПОДОБНО &СтрокаПоиска
	|   И //Отбор
	|
	|УПОРЯДОЧИТЬ ПО
	|	ВидыЗадач.Наименование";
	
	ОбъектМетаданных = Метаданные.Справочники.ВидыЗадач;
	Если Не МультиязычностьПовтИсп.КонфигурацияИспользуетТолькоОдинЯзык(ОбъектМетаданных.ТабличныеЧасти.Найти("Представления") = Неопределено) Тогда
		
		Если МультиязычностьСервер.ИспользуетсяПервыйДополнительныйЯзык() Тогда
			Запрос.Текст = СтрЗаменить(Запрос.Текст,
				"ВидыЗадач.Наименование ПОДОБНО &СтрокаПоиска",
				"(ВидыЗадач.Наименование ПОДОБНО &СтрокаПоиска ИЛИ ВидыЗадач.НаименованиеЯзык1 ПОДОБНО &СтрокаПоиска)");
		КонецЕсли;
		
	КонецЕсли;
	
	Отборы = Новый Массив;
	
	Если Параметры.Свойство("Отбор") Тогда
	
		Для Каждого Поле Из Параметры.Отбор Цикл
			
			Если ТипЗнч(Поле.Значение) = Тип("Массив")
				Или ТипЗнч(Поле.Значение) = Тип("ФиксированныйМассив") Тогда
				Отборы.Добавить(Поле.Ключ + " В (&" + Поле.Ключ + ")");
			Иначе
				Отборы.Добавить(Поле.Ключ + " = &" + Поле.Ключ);
			КонецЕсли;
			
			Запрос.УстановитьПараметр(Поле.Ключ, Поле.Значение);
			
		КонецЦикла;
		
	КонецЕсли;
		
	Если Отборы.Количество() Тогда
		Запрос.Текст = СтрЗаменить(Запрос.Текст, "//Отбор", СтрСоединить(Отборы, "
			|	И "));
	Иначе	
		
		Запрос.Текст = СтрЗаменить(Запрос.Текст, "И //Отбор", "");
		
	КонецЕсли;
	
	ДанныеВыбора = Новый СписокЗначений;
	Выборка = Запрос.Выполнить().Выбрать();
	Пока Выборка.Следующий() Цикл
		ДанныеВыбора.Добавить(Выборка.Ссылка);
	КонецЦикла;
	
КонецПроцедуры

