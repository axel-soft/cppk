	ВидЗадачиОбъект.ТерминНаправитьНаИсполнение = НСтр("ru = 'Направить на утверждение'");
	ВидЗадачиОбъект.ТерминНаправленаНаИсполнение = НСтр("ru = 'Направлена на утверждение'");
	ВидЗадачиОбъект.ТерминНеНаправленаНаИсполнение = НСтр("ru = 'Задача не направлена на утверждение. Сохранить задачу?'");
	ВидЗадачиОбъект.ШаблонЗаголовка = НСтр("ru = 'Утвердить'") + " ";
	ВидЗадачиОбъект.ВидУчастникаОтветственный = Справочники.ВидыУчастниковЗадач.ПустаяСсылка();
	ВидЗадачиОбъект.ВидУчастникаИсполнитель = Справочники.ВидыУчастниковЗадач.Утверждающий;
	ВидЗадачиОбъект.ВидУчастникаПроверяющий = Справочники.ВидыУчастниковЗадач.ОбрабатывающийРезультат;
	ВидЗадачиОбъект.ВидДействияИсполнителя = Справочники.ВидыДействийЗадач.Утвердить;
	ВидЗадачиОбъект.ВидДействияПроверяющего = Справочники.ВидыДействийЗадач.ОбработатьРезультат;
	ВидЗадачиОбъект.ДоступенДляВыбора = Ложь;
	ВидЗадачиОбъект.НаборСостояний = Справочники.НаборыСостоянийЗадач.Утверждение;
	
	ВидЗадачиОбъект.ВидыДействий.Очистить();
	
	СтрокаДействия = ВидЗадачиОбъект.ВидыДействий.Добавить();
	СтрокаДействия.ВидДействия = Справочники.ВидыДействийЗадач.Утвердить;
	
	СтрокаДействия = ВидЗадачиОбъект.ВидыДействий.Добавить();
	СтрокаДействия.ВидДействия = Справочники.ВидыДействийЗадач.ОбработатьРезультат;
	
