		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ТТС_ЖурналыТУ.ВнутреннийДокумент,
	ТТС_ЖурналыТУ.ДатаПоездки,
	ТТС_ЖурналыТУ.НомерПоезда,
	ТТС_ЖурналыТУ.Направление,
	ТТС_ЖурналыТУ.Машинист,
	ТТС_ЖурналыТУ.СерияЛокомотива,
	ТТС_ЖурналыТУ.НомерЛокомотива,
	ТТС_ЖурналыТУ.ЭДТ
ИЗ
	РегистрСведений.ТТС_ЖурналыТУ КАК ТТС_ЖурналыТУ
ГДЕ
	ТТС_ЖурналыТУ.ВидЖурнала = ЗНАЧЕНИЕ(Перечисление.ТТС_ВидыЖурналовТУ.Журнал1)
	И ТТС_ЖурналыТУ.ДатаПоездки МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И НЕ ТТС_ЖурналыТУ.ВнутреннийДокумент.ПометкаУдаления
	И НЕ ТТС_ЖурналыТУ.ВнутреннийДокумент.РегистрационныйНомер = ""</query>
