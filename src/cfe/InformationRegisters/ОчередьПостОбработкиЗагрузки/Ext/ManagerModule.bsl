
&ИзменениеИКонтроль("ДобавитьВОчередьНаборЗаписейРегистра")
Процедура ЦППК_ДобавитьВОчередьНаборЗаписейРегистра(Набор, ПричинаЗагрузки)

	ИОМССылка =  МиграцияДанныхИзВнешнихСистемСервер.ИОМ(
	Набор.Метаданные().ПолноеИмя(),
	Новый Соответствие);

	ЗначенияКлюча = Новый ХранилищеЗначения(Набор.Отбор, Новый СжатиеДанных(9));

#Удаление
	Ключ = ОтметкиВремени.Ключ(Набор);
#КонецУдаления
#Вставка
	// Процедура существует только в модуле события
	Ключ = ОтметкиВремениСобытия.Ключ(Набор);
#КонецВставки

	Если ЗначениеЗаполнено(Ключ) Тогда
		ДобавитьВОчередь(
		Ключ, ПричинаЗагрузки, ИОМССылка, ЗначенияКлюча);
	КонецЕсли;

КонецПроцедуры
